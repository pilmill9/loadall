        <xs:element name="Subspecies" type="SubspeciesType" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="SubspeciesType">
    <xs:sequence>
      <xs:group ref="tsngroup"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="SubkingdomType">
