          <xs:documentation>Subtypes of this rank</xs:documentation>
        </xs:annotation>
        <xs:element name="Subkingdom" nillable="false" type="SubkingdomType"/>
        <xs:element name="Infrakingdom" type="InfrakingdomType"/>
